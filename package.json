{
  "name": "w-rpi",
  "version": "0.1.5",
  "description": "Recursive Prompt Improver - A desktop/web application for testing, improving, and managing LLM prompts with multi-provider support",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "d-barletta",
    "email": "davide.barletta@icloud.com"
  },
  "main": "electron/main.js",
  "scripts": {
    "start": "BROWSER=none npm run poststart",
    "poststart": "node --max_old_space_size=4064 scripts/start.js",
    "build": "node --max_old_space_size=4064 scripts/build.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix --no-warn-ignored",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss,md}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,scss,md}\" \"*.{js,json,md}\"",
    "electron": "electron .",
    "electron:build": "npm run build && electron .",
    "electron:dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development ELECTRON_DEBUG=true electron .\"",
    "start:dev": "npm run electron:dev",
    "package": "electron-builder --publish never",
    "package:mac": "electron-builder --mac --publish never",
    "package:win": "electron-builder --win --publish never",
    "package:linux": "electron-builder --linux --publish never",
    "build:executable": "cross-env NODE_ENV=production npm run build && npm run package",
    "build:executable:mac": "cross-env NODE_ENV=production npm run build && npm run package:mac",
    "build:executable:win": "cross-env NODE_ENV=production npm run build && npm run package:win",
    "build:executable:linux": "cross-env NODE_ENV=production npm run build && npm run package:linux"
  },
  "dependencies": {
    "@alexbruf/react-diff-viewer": "^3.1.5",
    "@babel/core": "^7.28.5",
    "@carbon/icons-react": "^11.71.0",
    "@carbon/react": "^1.97.0",
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.8",
    "@fsegurai/codemirror-theme-github-dark": "^6.2.3",
    "@lobehub/icons-static-svg": "^1.74.0",
    "@moinfra/mcp-client-sdk": "^1.10.1-dev",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@svgr/webpack": "^8.1.0",
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "babel-loader": "^10.0.0",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.1.0",
    "bfj": "^9.1.2",
    "browserslist": "^4.28.1",
    "camelcase": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "codemirror": "^6.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "17.2.3",
    "dotenv-expand": "^12.0.3",
    "echarts-for-react": "^3.0.5",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "11.3.2",
    "html-webpack-plugin": "^5.6.5",
    "http-proxy-middleware": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "js-tiktoken": "^1.0.21",
    "jsonrepair": "^3.13.1",
    "lottie-react": "^2.4.1",
    "mini-css-extract-plugin": "^2.9.4",
    "moment": "^2.30.1",
    "pdfjs-dist": "^5.4.449",
    "postcss": "8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.2.0",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "prompts": "^2.4.2",
    "react": "^19.2.1",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-refresh": "^0.18.0",
    "react-router-dom": "^7.10.1",
    "remark-gfm": "^4.0.1",
    "resolve": "^1.22.11",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "semver": "7.7.3",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "tesseract.js": "^6.0.1",
    "vosk-browser": "^0.0.8",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^7.4.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "globals": "^16.5.0",
    "wait-on": "^9.0.3",
    "webpack-sources": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "build": {
    "appId": "com.rpi.app",
    "productName": "RPI",
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "build/android-chrome-512x512.png"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "icon": "public/android-chrome-512x512.png",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development",
      "icon": "public/android-chrome-512x512.png"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "electronLanguages": [
        "en"
      ],
      "extendInfo": {
        "CFBundleDisplayName": "RPI",
        "CFBundleName": "RPI",
        "CFBundleIdentifier": "com.rpi.app"
      }
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
